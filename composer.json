{
    "name": "jcrodriguezt/laravel-sybase",
    "description": "Sybase based Eloquent module extension for Laravel > 7.x.",
    "keywords": [
        "sybase"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Julio C. Rodriguez",
            "email": "jcrodsolutions@gmail.com",
			"role" : "Developer"
        },
        {
            "name": "Gabriel Tadra Mainginski",
            "email": "13179626@uepg.br",
			"role" : "Original Developer"
        },
        {
            "name": "Ademir Mazer Junior",
            "email": "mazer@uepg.br",
			"role" : "Original Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jcrodriguezt/laravel-sybase/issues"
    },
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^3.5",
        "illuminate/database": "8.*|9.*|10.*",
	"illuminate/support": "~8.0|~9.0|~10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uepg\\LaravelSybase\\SybaseServiceProvider"
            ],
            "aliases": {
                "UepgBlueprint": "Uepg\\LaravelSybase\\Database\\Schema\\Blueprint"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Uepg\\LaravelSybase\\": "src/"
        }
    }
}
